{
  "version": 2,
  "builds": [
    {
      "src": "public/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "dist/index.js"
    },
    {
      "source": "/(.+)",
      "destination": "dist/index.js"
    }
  ],
  "redirects": [
    {
      "source": "/",
      "destination": "https://github.com/biaov/github-statistics"
    }
  ],
  "cleanUrls": true
}
